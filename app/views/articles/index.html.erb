<%= link_to "back", :back %>
<%= link_to "write", new_article_path %>

<h2>Articles#index</h2>

<h3>Here are all articles</h3>

<% @articles.each do |item| %>
  <%= link_to article_path(item) do %>
    <p><%= item.title %> - <small>by: <%= item.user == current_user ? "you wrote this article!" : item.user.username %></small></p>
  <% end %>
<% end %>
